{
  "name": "vpn-tg-bot",
  "version": "1.0.0",
  "description": "Telegram бот для управления доступом к VPN Xray с обработкой платежей через ЮKassa",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "build:ignore": "tsc --skipLibCheck --noEmitOnError false",
    "build:admin": "vite build --config vite.config.ts",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:admin": "vite --config vite.config.ts",
    "deploy": "ts-node src/scripts/deploy.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "telegram",
    "bot",
    "vpn",
    "xray",
    "subscription",
    "yookassa"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@prisma/client": "^5.12.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.12.12",
    "@types/node-telegram-bot-api": "^0.63.3",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^8.2.3",
    "node-telegram-bot-api": "^0.65.1",
    "nodemon": "^3.1.0",
    "prisma": "^5.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "vite": "^5.1.6",
    "winston": "^3.12.0",
    "yookassa": "^0.1.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "jimp": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^3.3.6",
    "qrcode": "^1.5.4",
    "cron": "^2.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
